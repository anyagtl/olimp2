#include <bits/stdc++.h>

int m, k, o;
std::vector <int> x, y; 
std::string s;
std::cin >> s;
std::cin >> m >> k;
for (int i = 0; i < m; i++) {
    std::cin >> o;
}

for (int it = 0; it < s.size(); it++) {

x = {0, 1, 2, 3, 4, 5 ,6 ,7}
y = {0, 1, 2, 3, 4, 5, 6, 7}
for (int i = 0; i < x.size(); i++) {
    for (int j = 0; j < y.size(); j++) {
        std::cout << s[it] << '/n' << s[i] << '/n' << s[j];
    }
}


